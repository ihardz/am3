{
  "version": 1,
  "projects": {
    "layout": {
      "projectType": "library",
      "root": "libs/layout",
      "sourceRoot": "libs/layout/src",
      "prefix": "am3",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/layout/tsconfig.lib.json",
              "libs/layout/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/layout/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/layout/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      }
    },
    "ionic-am": {
      "root": "apps/ionic-am/",
      "sourceRoot": "apps/ionic-am/src",
      "projectType": "application",
      "prefix": "am3",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "apps/ionic-am/www",
            "index": "apps/ionic-am/src/index.html",
            "main": "apps/ionic-am/src/main.ts",
            "polyfills": "apps/ionic-am/src/polyfills.ts",
            "tsConfig": "apps/ionic-am/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "apps/ionic-am/src/assets",
                "output": "assets"
              },
              {
                "glob": "**/*.svg",
                "input": "node_modules/ionicons/dist/ionicons/svg",
                "output": "./svg"
              }
            ],
            "styles": [
              {
                "input": "apps/ionic-am/src/theme/variables.scss"
              },
              {
                "input": "apps/ionic-am/src/global.scss"
              }
            ],
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": [
                "_mixins.scss"
              ]
            }
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ionic-am/src/environments/environment.ts",
                  "with": "apps/ionic-am/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "ci": {
              "progress": false
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ionic-am:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ionic-am:build:production"
            },
            "ci": {
              "progress": false
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ionic-am:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/ionic-am/src/test.ts",
            "polyfills": "apps/ionic-am/src/polyfills.ts",
            "tsConfig": "apps/ionic-am/tsconfig.spec.json",
            "karmaConfig": "apps/ionic-am/karma.conf.js",
            "styles": [],
            "scripts": [],
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "apps/ionic-am/src/",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "apps/ionic-am/src/assets",
                "output": "/assets"
              }
            ]
          },
          "configurations": {
            "ci": {
              "progress": false,
              "watch": false
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/ionic-am/tsconfig.app.json",
              "apps/ionic-am/tsconfig.spec.json",
              "apps/ionic-am/e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "ionic-cordova-build": {
          "builder": "@ionic/angular-toolkit:cordova-build",
          "options": {
            "browserTarget": "ionic-am:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ionic-am:build:production"
            }
          }
        },
        "ionic-cordova-serve": {
          "builder": "@ionic/angular-toolkit:cordova-serve",
          "options": {
            "cordovaBuildTarget": "ionic-am:ionic-cordova-build",
            "devServerTarget": "ionic-am:serve"
          },
          "configurations": {
            "production": {
              "cordovaBuildTarget": "ionic-am:ionic-cordova-build:production",
              "devServerTarget": "ionic-am:serve:production"
            }
          }
        }
      }
    },
    "meter": {
      "projectType": "library",
      "root": "libs/meter",
      "sourceRoot": "libs/meter/src",
      "prefix": "am3",
      "architect": {
        "lint": {
