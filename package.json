    "@nstudio/ionic": "*",
    "@nstudio/angular": "*",
    "@nstudio/web-angular": "*",
    "@angular-devkit/architect": "~0.1000.6",
    "@angular-devkit/core": "~10.1.0",
    "@angular-devkit/schematics": "~10.1.0",
    "@ionic/angular-toolkit": "^2.3.0",
    "@types/jasmine": "~3.5.9",
    "@types/jasminewd2": "~2.0.3",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
