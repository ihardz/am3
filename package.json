    "@nrwl/cypress": "10.2.1",
    "@nrwl/jest": "10.2.1",
    "@nrwl/workspace": "10.2.1",
    "@nstudio/xplat": "^10.1.1",
    "@types/jest": "26.0.8",
    "@types/node": "~8.9.4",
    "codelyzer": "~6.0.0",
